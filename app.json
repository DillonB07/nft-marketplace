{
  "expo": {
    "name": "ProNef",
    "slug": "ProNef",
    "description": "ProNef NFT Marketplace. Make sure to install the Expo Go app before scanning the QR code inside of the app.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ProNef.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ProNef.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
